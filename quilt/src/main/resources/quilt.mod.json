{
  "schema_version": 1,
  "mixin": [
    "better_resource_pack_sorting.mixins.json",
    "better_resource_pack_sorting-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "better_resource_pack_sorting",
    "version": "${version}",
    "metadata": {
      "name": "Better Resource Pack Sorting",
      "description": "${description}",
      "contributors": {
        "MrMelon54": "Owner"
      },
      "contact": {
        "homepage": "https://mrmelon54.com/minecraft/better-resource-pack-sorting",
        "sources": "https://github.com/mrmelon54/better_resource_pack_sorting",
        "issues": "https://github.com/mrmelon54/better_resource_pack_sorting/issues"
      },
      "license": "GPL-3.0-only",
      "icon": "icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.mrmelon54.BetterResourcePackSorting.quilt.BetterResourcePackSortingQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "${minecraft_version}"
      },
      {
        "id": "architectury",
        "version": ">=${architectury_version}"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}
